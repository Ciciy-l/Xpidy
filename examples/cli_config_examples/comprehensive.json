{
  "spider_config": {
    "headless": true,
    "timeout": 30000,
    "stealth_mode": true,
    "random_delay": true,
    "min_delay": 0.5,
    "max_delay": 2.0
  },
  "extraction_config": {
    "extract_text": true,
    "extract_links": true,
    "extract_images": true,
    "extract_metadata": true,
    "extract_structured_data": true,
    "extract_tables": true,
    "extract_forms": true,
    "max_links": 50,
    "max_images": 20,
    "min_image_width": 100,
    "min_image_height": 100,
    "image_formats": ["jpg", "png", "gif", "webp"],
    "normalize_whitespace": true,
    "enable_deduplication": true
  },
  "tasks": [
    {
      "url": "https://httpbin.org/html",
      "name": "comprehensive_extraction",
      "options": {
        "deduplicate": true,
        "include_media": false,
        "exclude_patterns": [".*\\.pdf$", ".*\\.zip$"]
      }
    }
  ]
} 